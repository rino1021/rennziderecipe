<div class ="container px-5 px-sm-0">
   <div class = "row">
    <div class ="col-md-2"></div>
    <div class ="col-md-10">
<h1>Users</h1>
      <table class="table">
        <tr>
          <th>image</th><th>name</th>
          <th colspan="3"></th>
        </tr>
      <table class="table">
      <tbody>
       <% @users.each do |user| %>
        <tr>
          <td><%= image_tag user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", admins_user_path(user.id) %></td>
        </tr>
       <% end %>
        </tbody>
      </table>
      </div>